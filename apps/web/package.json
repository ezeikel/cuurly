{
  "name": "@cuurly/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check . --ignore-path ../../.gitignore --cache",
    "format": "prettier --write . --ignore-path ../../.gitignore --cache",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "pw:run": "playwright test",
    "pw:open": "playwright test --ui",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 'playwright test'",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 'playwright test --ui'",
    "pw:codegen": "playwright codegen http://localhost:3000",
    "with-env": "dotenv -e ../../.env --",
    "db:generate": "pnpm -F db db:generate",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.+(js|jsx|json|ts|tsx|css|html|md)": [
      "prettier --ignore-path .gitignore --write",
      "jest --findRelatedTests --passWithNoTests"
    ]
  },
  "dependencies": {
    "@auth/core": "^0.37.4",
    "@cuurly/db": "workspace:*",
    "@cuurly/sentry-config": "workspace:*",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.6.0",
    "@fortawesome/pro-light-svg-icons": "^6.6.0",
    "@fortawesome/pro-regular-svg-icons": "^6.6.0",
    "@fortawesome/pro-solid-svg-icons": "^6.6.0",
    "@fortawesome/pro-thin-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@sentry/nextjs": "^8.35.0",
    "@tanstack/react-query": "^5.59.16",
    "@testing-library/dom": "^10.4.0",
    "@vercel/analytics": "^1.3.2",
    "@vercel/functions": "^1.4.2",
    "axios": "^1.7.7",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.5.1",
    "cloudinary": "^2.5.1",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "formik": "^2.4.6",
    "heic2any": "^0.0.4",
    "jsonwebtoken": "^9.0.2",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "next": "15.0.4",
    "next-auth": "beta",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "^14.2.10",
    "react-modal": "^3.16.1",
    "react-responsive": "^10.0.0",
    "react-select": "^5.8.2",
    "react-textarea-autosize": "^8.5.4",
    "react-toastify": "^10.0.6",
    "video.js": "^8.19.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@cuurly/eslint-config": "workspace:*",
    "@cuurly/prettier-config": "workspace:*",
    "@cuurly/tsconfig": "workspace:*",
    "@playwright/test": "^1.48.2",
    "@prisma/nextjs-monorepo-workaround-plugin": "^6.0.1",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^22.8.1",
    "@types/react": "19.0.1",
    "@types/react-fontawesome": "^1.6.8",
    "@types/react-modal": "^3.16.3",
    "@types/react-responsive": "^8.0.8",
    "@types/react-textarea-autosize": "^8.0.0",
    "@types/video.js": "^7.3.58",
    "@types/yup": "^0.32.0",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.16.0",
    "globals": "latest",
    "husky": "^9.1.6",
    "is-ci-cli": "^2.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "latest"
  }
}
